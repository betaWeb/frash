{
  "name": "alixsperoza/lfw",
  "description": "Small PHP Framework - MVC, based on TWIG",
  "keywords": [ "LFW", "Framework", "MVC" ],
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "twig/twig": "1.24.1"
  },
  "autoload": {
    "classmap": [ ".htaccess", "console.php", "index.php", "Configuration/.htaccess", "Configuration/config.json", "Configuration/database.json", "Configuration/dependencies.json", "Configuration/routing.json" ],
    "psr-4": {
      "Bundles\\": "Bundles/",
      "Traductions\\": "Traductions/",
      "LFW\\": "vendor/LFW/"
    }
  }
}