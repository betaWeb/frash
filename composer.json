{
  "name": "alixsperoza/lfw",
  "description": "Small PHP Framework - MVC, based on TWIG",
  "keywords": [ "LFW", "Framework", "MVC" ],
  "license": "MIT",
  "minimum-stability": "dev-master",
  "require": {
    "twig/twig": "1.24.1",
    "symfony/yaml": "3.1.5"
  },
  "autoload": {
    "classmap": [ ".htaccess", "console", "index.php" ],
    "psr-4": {
      "Bundles\\": "Bundles/",
      "Traductions\\": "Traductions/",
      "LFW\\": "vendor/LFW/"
    }
  }
}